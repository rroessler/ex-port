{
    "name": "ext-port",
    "version": "1.1.5",
    "description": "Extensible Serial-Port Implementations",
    "main": "dist/index.js",
    "types": "typings/index.d.ts",
    "files": [
        "dist/**/*",
        "typings/**/*"
    ],
    "scripts": {
        "prepublishOnly": "npm run dist",
        "dist": "npm run clean && tsc",
        "test": "ava",
        "cover": "nyc npm run test",
        "clean": "rimraf dist typings"
    },
    "keywords": [
        "abstraction",
        "COM",
        "COM port",
        "modbus",
        "port",
        "serial",
        "serialport",
        "serial port",
        "stream",
        "tty",
        "typescript"
    ],
    "author": "Reuben Roessler",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/rroessler/ext-port.git"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@types/sinon": "^10.0.11",
        "ava": "^4.3.0",
        "nyc": "^15.1.0",
        "rimraf": "^3.0.2",
        "sinon": "^14.0.0",
        "ts-node": "^10.8.1",
        "typescript": "^4.7.3"
    },
    "dependencies": {
        "serialport": "^10.4.0",
        "ts-monadable": "^1.0.2"
    },
    "ava": {
        "files": [
            "lib/**/*.test.ts",
            "!typings/**/*",
            "!node_modules/**/*"
        ],
        "extensions": [
            "ts"
        ],
        "require": [
            "ts-node/register"
        ]
    }
}
